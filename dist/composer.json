{
    "config": {
        "vendor-dir": "vendor",
        "optimize-autoloader": true
    },

    "require": {
        "components/jquery": "~3.6.0",
        "components/font-awesome": "4.7.0",
        "twbs/bootstrap": "4.3.1",
        "twig/twig": "^3.0",
        "csstools/sanitize.css": "13.0.0",
        "necolas/normalize.css": "*",
        "murtaugh/html5-reset": "*"
    },
    "extra": {
        "ajgl-symlinks": {
            "twbs/bootstrap": {
                "dist/css": "web/assets/css/bootstrap",
                "dist/js": "web/assets/js/bootstrap",
                "dist/fonts/glyphicons-halflings-regular.eot": "web/assets/fonts/glyphicons-halflings-regular.eot",
                "dist/fonts/glyphicons-halflings-regular.svg": "web/assets/fonts/glyphicons-halflings-regular.svg",
                "dist/fonts/glyphicons-halflings-regular.ttf": "web/assets/fonts/glyphicons-halflings-regular.ttf",
                "dist/fonts/glyphicons-halflings-regular.woff": "web/assets/fonts/glyphicons-halflings-regular.woff",
                "dist/fonts/glyphicons-halflings-regular.woff2": "web/assets/fonts/glyphicons-halflings-regular.woff2"
            }
        }
    },
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "necolas/normalize.css",
                "version":"2",
                "source": {
                    "url": "https://github.com/necolas/normalize.css.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "name": "murtaugh/html5-reset",
                "version":"1",
                "source": {
                    "url": "https://github.com/murtaugh/html5-reset.git",
                    "type": "git",
                    "reference":"master"
                }
            }
        },
        {
            "type":"package",
            "package": {
                "name": "csstools/sanitize.css",
                "version":"13.0.0",
                "source": {
                    "url": "https://github.com/csstools/sanitize.css.git",
                    "type": "git",
                    "reference":"main"
                }
            }
        }
    ]
}
