
    <!-- jQuery, via CDN + fallback to local copy if CDN fail -->
    <script
        src="https://code.jquery.com/jquery-{{config.jquery.version}}.min.js"
        integrity="{{config.jquery.integrity}}"
        crossorigin="anonymous"></script>
    
    <!-- Failsafe jQuery Loader - Local copy -->
    <script>window.jQuery || document.write('<script src="{{js}}/jquery-{{config.jquery.version}}.min.js" type="text/javascript">\x3C/script>')</script>
