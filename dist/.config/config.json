{
    "environment": {
        "types": ["DEV", "TEST", "STAGE", "PRODUCTION"],
        "current": "DEV",
        "simulated": "PRODUCTION_2"
    },
    "maintenance": false,
    "composer": true,
    "static": {
        "maintenance": "maintenance.html",
        "welcome": "welcome.html",
        "error": "error.html"
    },
    "twig": {
        "install": true,
        "fullpath": false,
        "template": "boilerplate",
        "maintenance": "maintenance.php",
        "welcome": "welcome.php",
        "error": "error.php"
    },
    "debug": {
        "error_reporting": 32767,
        "enabled": false,
        "output": "log"
    },
    "cache": {
        "enabled": true,
        "folder": ".cache"
    },
    "log": {
        "enabled": true,
        "folder": ".log"
    },
    "temp": {
        "enabled": true,
        "folder": ".temp"
    },
    "timezone": "America/Detroit",
    "country": "US",
    "language": "en",
    "charset": "utf-8",
    "extensions": {
        "allowed": ["html", "php"],
        "blocked": [ ],
        "default": "php",
        "empty": true
    },
    "index": "index.php"
}